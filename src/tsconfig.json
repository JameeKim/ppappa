{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": [
      "es2015",
      "dom"
    ],
    "noEmitOnError": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "newLine": "lf",
    "noImplicitReturns": true,
    "strict": true
  },
  "include": [
    "./**/*.ts"
  ]
}
